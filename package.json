{
  "name": "vue3-q-tel-input",
  "version": "1.0.10",
  "description": "Vue 3 telephone input implemented over Quasar framework",
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "rm -rf dist && cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "keywords": [
    "vue3", "vue-3", "vue 3", "Vue3", "Vue 3", "Quasar", "Quasar V2", "Quasar Framework", "Quasar Framweork V3",
    "Country", "Input", "Country Input", "Telephone", "Tel Input", "Telephon Input", "International Number", "International",
    "Phone Number", "Phone", "q-tel", "q-tel-input", "Q Tel", "Q Tel Input", "Quasar telephone", "Quasar Telephone Input",
    "Quasar Phone Number", "Quasar Phone", "Quasar Phone Number Input", "Vue Phone Number", "Vue Phone Input", "Vue Phone International"
  ],
  "main": "dist/vue3-q-tel-input.ssr.js",
  "module": "dist/vue3-q-tel-input.esm.js",
  "browser": "dist/vue3-q-tel-input.esm.js",
  "unpkg": "dist/vue3-q-tel-input.min.js",
  "files": [
    "dist/*",
    "vue3-q-tel-input.d.ts",
    "src/**/*.vue"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@quasar/extras": "^1.0.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/google-libphonenumber": "^7.4.21",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "@vue/compiler-sfc": "^3.0.5",
    "cross-env": "^7.0.3",
    "google-libphonenumber": "^3.2.21",
    "minimist": "^1.2.5",
    "postcss": "^8.2.3",
    "quasar": "^2.0.0",
    "rollup": "^2.36.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-ignore": "^1.0.9",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "1.32.12",
    "sass-loader": "^10.1.0",
    "typescript": "^3.8.3",
    "vue": "^3.0.5",
    "vue-class-component": "^8.0.0-rc.1",
    "vue-cli-plugin-quasar": "~4.0.1",
    "vue-property-decorator": "^10.0.0-rc.3"
  },
  "peerDependencies": {
    "quasar": "^2.0.0",
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=12"
  },
  "sideEffects": false,
  "license": "MIT",
  "types": "vue3-q-tel-input.d.ts"
